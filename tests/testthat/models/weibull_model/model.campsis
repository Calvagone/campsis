[MAIN]
TVTAU=THETA_TAU
TVSH=THETA_SH
TVCL=THETA_CL
TVVC=THETA_VC
TVPROP_RUV=THETA_PROP_RUV

TAU=TVTAU * exp(ETA_TAU)
SH=TVSH * exp(ETA_SH)
CL=TVCL * exp(ETA_CL)
VC=TVVC * exp(ETA_VC)
PROP_RUV=TVPROP_RUV

[ODE]
WF=DOSE * exp(-pow((t-TDOS)/TAU, SH)) * (SH/TAU) * pow((t-TDOS)/TAU, SH-1)
d/dt(A_ABS)=-WF*A_ABS
d/dt(A_CENTRAL)=WF*A_ABS - CL/VC*A_CENTRAL

[ERROR]
CONC=A_CENTRAL/VC
IPRED=log(0.001)
if (CONC > 0) IPRED=log(CONC)
W=PROP_RUV
Y=IPRED + W*EPS_RUV_FIX

